#整数11转换为字符串类型
string_name = str(11)
print (type(string_name), string_name)

#将字符串转换为数字
number = int("11")
print(type(number), number)

number2 = float("11.11")
print(type(number2), number2)

#整数转浮点数
float_num = float(13)
print(type(float_num), float_num)

#浮点数转整数
int_num = int(13.93)
print(type(int_num), int_num)
